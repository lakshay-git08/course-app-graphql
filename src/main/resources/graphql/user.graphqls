type User {
  id: ID!
  name: String!
  email: String!
}

input UserInput {
  name: String!
  email: String!
  roleType: Int!
  username: String!
  password: String!
}

input UpdateUserInput {
  name: String
  email: String
}

input UserFilter {
  page: Int
  limit: Int
  sortBy: UserSortBy
  sortOrder: UserSortOrder
}

enum UserSortBy {
  EMAIL
  NAME
}
enum UserSortOrder {
  ASC
  DESC
}
